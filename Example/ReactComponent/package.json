{
  "name": "RNDemo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "sync": "cp -r ./dist/* ../QJRouter/rndist/",
    "build": "react-native bundle --entry-file index.tsx --bundle-output ./dist/index.ios.jsbundle --platform ios --assets-dest ./dist --dev false --minify false",
    "detox-test-ios": "detox test --loglevel verbose --configuration ios.sim.debug --debug-synchronization 1000",
    "detox-test-android": "detox test --loglevel verbose --configuration android.emu.debug --debug-synchronization 1000",
    "sb": "start-storybook -p 7007",
    "build-sb": "build-storybook"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-native": "0.62.2",
    "react-native-video": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.2",
    "@types/react-native": "^0.63.50",
    "@types/react-native-video": "^5.0.4",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-root-import": "^6.6.0",
    "detox": "^18.6.2",
    "eslint": "^7.20.0",
    "jest": "^26.6.3",
    "react-dom": "16.13.1",
    "react-test-renderer": "16.13.1",
    "typescript": "^4.1.5"
  },
  "test-runner": "jest"
}
